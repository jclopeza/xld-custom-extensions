<synthetic xsi:schemaLocation="http://www.xebialabs.com/deployit/synthetic synthetic.xsd" xmlns="http://www.xebialabs.com/deployit/synthetic" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <type-modification type="udm.Environment">
        <property name="requiresOkTestManager" description="Ok test manager is required" kind="boolean" required="false" category="Deployment Checklist" />
        <property name="requiresOkReleaseManager" description="Ok release manager is required" kind="boolean" required="false" category="Deployment Checklist" />
    </type-modification>
    <type-modification type="udm.Version">
        <property name="satisfiesOkTestManager" description="Ok test manager" kind="boolean" required="false" category="Deployment Checklist" />
        <property name="satisfiesOkReleaseManager" description="Ok release manager" kind="boolean" required="false" category="Deployment Checklist" />
    </type-modification>
    
    <type type="axis2.Deployer" extends="generic.Container">
        <property name="axis2ServicesDirectory" />
    </type>
    <type type="axis2.DeployedAar" extends="udm.BaseDeployedArtifact" deployable-type="axis2.Aar" container-type="axis2.Deployer">
        <generate-deployable type="axis2.Aar" extends="generic.File" />
        <property name="targetFileName" />
    </type>

</synthetic>
